{
  "name": "create-moji-tauri",
  "type": "module",
  "version": "0.1.2-beta.1",
  "packageManager": "pnpm@9.14.4",
  "description": "A CLI tool for quickly generating Tauri project templates",
  "author": "Clover You",
  "license": "MIT",
  "homepage": "https://github.com/moji-open-source/tauri-create#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moji-open-source/tauri-create.git"
  },
  "bugs": "https://github.com/moji-open-source/tauri-create/issues",
  "keywords": [
    "cli",
    "tauri",
    "generate",
    "rust"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "bin": {
    "create-moji-tauri": "./bin/create.mjs"
  },
  "files": [
    "dist",
    "templates"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "tsx src/commands/create.ts",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && pnpm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "@posva/prompts": "^2.4.4",
    "picocolors": "^1.1.1",
    "tinyglobby": "^0.2.10",
    "tsx": "^4.19.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.11.2",
    "@antfu/ni": "^0.23.1",
    "@antfu/utils": "^0.7.10",
    "@types/minimist": "^1.2.5",
    "@types/node": "^22.10.1",
    "bumpp": "^9.8.1",
    "eslint": "^9.16.0",
    "esno": "^4.8.0",
    "lint-staged": "^15.2.10",
    "minimist": "^1.2.8",
    "pnpm": "^9.14.4",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.7.2",
    "unbuild": "^2.0.0",
    "vite": "^6.0.2",
    "vitest": "^2.1.8"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
